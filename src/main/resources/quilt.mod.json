{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "ipauth",
    "version": "${version}",
    "metadata": {
      "name": "IPAuth",
      "description": "A server-side Minecraft Quilt mod that enables per user IP authorization.",
      "contributors": {
        "krystejj": "Owner"
      },
      "license": "MIT"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "server_init": "me.krystejj.ipauth.IPAuth"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.1"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.0.2"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20"
      }
    ]
  }
}
